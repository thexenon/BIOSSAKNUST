{
  "expo": {
    "scheme": "biossa",
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "name": "BIOSSA KNUST",
    "slug": "biossa-knust",
    "icon": "./src/assets/images/logo.jpg",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thexenon.biossa_knust",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/logo.jpg"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d63a8a7a-101b-4808-b9f5-f4cffc00bb98"
      }
    },
    "ios": {
      "bundleIdentifier": "com.thexenon.biossaknust",
      "infoPlist": {
        "NSCameraUsageDescription": "We need camera access to take photos you choose to upload.",
        "NSPhotoLibraryUsageDescription": "We need access to your photos so you can select images to upload.",
        "NSPhotoLibraryAddUsageDescription": "We need permission to save images to your photo library."
      }
    }
  }
}
